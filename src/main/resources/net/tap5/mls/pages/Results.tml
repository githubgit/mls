<t:layout title="" xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd" xmlns:p="tapestry:parameter">
   
   <t:tabGroup active="resultTab">
      <t:Loop t:source="tabs" value="tabItem">
        <t:tab name="${tabItem.name}" label="${tabItem.label}">${tabItem.content}</t:tab>
      </t:Loop>
   </t:tabGroup>
   
   
</t:layout>